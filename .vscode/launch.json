{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Server",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}\\index.mjs"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Perftest",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}\\test\\perftest.mjs"
		},
		{
			"name": "Launch SplitSprites",
			"type": "go",
			"request": "launch",
			"mode": "debug",
			"cwd": "${workspaceFolder}\\.private",
			"program": "${workspaceFolder}\\.private\\SplitSprites.go",
			"args": ["-i", "ManicTrafficSprites.plist"]
		}
	]
}
